<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DartTraining.Resources"
					xmlns:menu="clr-namespace:DartTraining.Menu"
                    xmlns:login="clr-namespace:DartTraining.Login">
    <DataTemplate DataType="{x:Type menu:MenuViewModel}">
		<menu:MenuUserControl></menu:MenuUserControl>
	</DataTemplate>
	<DataTemplate DataType="{x:Type login:LoginViewModel}">
		<login:LoginUserControl></login:LoginUserControl>
	</DataTemplate>
</ResourceDictionary>
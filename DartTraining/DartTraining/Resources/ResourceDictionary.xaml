<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:menu="clr-namespace:DartTraining.Menu"
                    xmlns:login="clr-namespace:DartTraining.Login"
                    xmlns:match="clr-namespace:DartTraining.Match">
    <DataTemplate DataType="{x:Type menu:MenuViewModel}">
		<menu:MenuUserControl></menu:MenuUserControl>
	</DataTemplate>
	<DataTemplate DataType="{x:Type login:LoginViewModel}">
		<login:LoginUserControl></login:LoginUserControl>
	</DataTemplate>
	<DataTemplate DataType="{x:Type match:MatchConfigViewModel}">
		<match:MatchConfigUserControl></match:MatchConfigUserControl>
	</DataTemplate>
	<DataTemplate DataType="{x:Type match:StatisticViewModel}">
		<match:StatisticView></match:StatisticView>
	</DataTemplate>
	<DataTemplate DataType="{x:Type match:MatchViewModel}">
		<match:MatchView></match:MatchView>
	</DataTemplate>
</ResourceDictionary>